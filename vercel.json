{
    "version": 2,
    "builds": [
        {
            "src": "backend/api/main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/api/main.py"
        }
    ],
    "env": {
        "PYTHONPATH": "/var/task/backend"
    }
} 