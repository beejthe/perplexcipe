{
    "version": 2,
    "builds": [
        {
            "src": "backend/api/debug.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9",
                "handler": "app"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/debug",
            "dest": "backend/api/debug.py"
        }
    ],
    "env": {
        "PYTHONPATH": "/var/task/backend"
    }
} 